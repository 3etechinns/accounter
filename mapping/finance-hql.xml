<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.vimukti.accounter.core"
	default-access="field" auto-import="false">
	<!-- <joined-subclass name="com.vimukti.accounter.workspace.tool.FinanceTool" 
		table="FINANCETOOL" extends="com.bizantra.server.workspace.ext.AbstractTool"> 
		<key column="ID"></key> <many-to-one name="company" lazy="false" class="Company" 
		column="COMPANY_ID" cascade="save-update,delete"> </many-to-one> </joined-subclass> -->


	<!-- Named HQL for Finance Tool -->

	<query name="list.TAXItem">from com.vimukti.accounter.core.TAXItem </query>

	<query name="list.Account">from com.vimukti.accounter.core.Account a order by
		a.number</query>
	<query name="list.CommodityCode">from com.vimukti.accounter.core.CommodityCode
	</query>
	<query name="list.AccountTransaction">from
		com.vimukti.accounter.core.AccountTransaction
	</query>
	<query name="list.FiscalYear">from com.vimukti.accounter.core.FiscalYear fy
		order by
		fy.startDate</query>
	<query name="list.Bank">from com.vimukti.accounter.core.Bank b order by
		b.name</query>
	<query name="list.Budget">from com.vimukti.accounter.core.Budget</query>
	<query name="list.Company">from com.vimukti.accounter.core.Company c order by
		c.fullName</query>
	<query name="list.CreditRating">from com.vimukti.accounter.core.CreditRating cr order
		by cr.name
	</query>
	<query name="list.Currency">from com.vimukti.accounter.core.Currency c order by
		c.name</query>
	<query name="list.CustomerGroup">from com.vimukti.accounter.core.CustomerGroup cg
		order by cg.name
	</query>
	<query name="list.Item">from com.vimukti.accounter.core.Item i order by
		i.name</query>
	<query name="list.ItemGroup">from com.vimukti.accounter.core.ItemGroup ig order by
		ig.name</query>
	<query name="list.Payee">from com.vimukti.accounter.core.Payee p order by
		p.name
	</query>
	<query name="list.PaymentTerms">from com.vimukti.accounter.core.PaymentTerms pt order
		by pt.name
	</query>
	<query name="list.PriceLevel">from com.vimukti.accounter.core.PriceLevel pl order
		by pl.name
	</query>
	<query name="list.ShippingMethod">from com.vimukti.accounter.core.ShippingMethod sm
		order by sm.name
	</query>
	<query name="list.ShippingTerms">from com.vimukti.accounter.core.ShippingTerms st
		order by st.name
	</query>
	<query name="list.TAXCode">from com.vimukti.accounter.core.TAXCode tc order by
		tc.name</query>

	<query name="list.TAXGroup">from com.vimukti.accounter.core.TAXGroup </query>
	<query name="list.TaxRates">from com.vimukti.accounter.core.TaxRates</query>

	<query name="list.TAXItemGroups">from com.vimukti.accounter.core.TAXItemGroup</query>
	<!-- <query name="list.TaxRateCalculation">from com.vimukti.accounter.core.TaxRateCalculation 
		</query> -->
	<query name="list.UnitOfMeasure">from com.vimukti.accounter.core.UnitOfMeasure u order
		by u.name
	</query>
	<query name="list.User">from com.vimukti.accounter.core.User u order by
		u.firstName,u.lastName</query>
	<query name="list.RecurringTransaction">from com.vimukti.accounter.core.RecurringTransaction
		rt order by
		rt.name</query>
	<query name="list.currentRecTransactions">from com.vimukti.accounter.core.RecurringTransaction rt where rt.nextScheduleOn = ? order by
		rt.name</query>
	<query name="list.VendorGroup">from com.vimukti.accounter.core.VendorGroup vg order
		by vg.name
	</query>
	<query name="list.Transaction">from com.vimukti.accounter.core.Transaction
	</query>
	<query name="list.Entry">from com.vimukti.accounter.core.Entry</query>
	<query name="list.TransactionItem">from com.vimukti.accounter.core.TransactionItem
	</query>
	<query name="list.CreditsAndPayments">from
		com.vimukti.accounter.core.CreditsAndPayments
	</query>
	<query name="list.TransactionCreditsAndPayments">from
		com.vimukti.accounter.core.TransactionCreditsAndPayments</query>
	<query name="list.TransactionMakeDeposit">from
		com.vimukti.accounter.core.TransactionMakeDeposit</query>
	<query name="list.TransactionPayBill">from
		com.vimukti.accounter.core.TransactionPayBill
	</query>
	<query name="list.TransactionReceivePayment">from
		com.vimukti.accounter.core.TransactionReceivePayment</query>
	<query name="list.TransactionIssuePayment">from
		com.vimukti.accounter.core.TransactionIssuePayment</query>
	<query name="list.TransactionPaySalesTax">from
		com.vimukti.accounter.core.TransactionPaySalesTax</query>
	<query name="list.PaySalesTaxEntries">from
		com.vimukti.accounter.core.PaySalesTaxEntries
	</query>
	<query name="list.TransactionMakeDepositEntries">from
		com.vimukti.accounter.core.TransactionMakeDepositEntries</query>
	<query name="list.TransactionExpense">from
		com.vimukti.accounter.core.TransactionExpense
	</query>
	<query name="list.TransactionPayExpense">from
		com.vimukti.accounter.core.TransactionPayExpense
	</query>
	<query name="list.Vendor">from com.vimukti.accounter.core.Vendor</query>
	<query name="list.Customer">from com.vimukti.accounter.core.Customer</query>
	<query name="list.SalesPerson">from com.vimukti.accounter.core.SalesPerson sp order
		by sp.firstName
	</query>
	<query name="list.TAXAgency">from com.vimukti.accounter.core.TAXAgency</query>
	<query name="list.FixedAsset">from com.vimukti.accounter.core.FixedAsset
	</query>
	<!-- <query name="list.SellingOrDisposingFixedAsset">from com.vimukti.accounter.core.SellingOrDisposingFixedAsset</query> -->
	<query name="list.VATReturn">from com.vimukti.accounter.core.VATReturn vr order by
		vr.taxAgency.name, vr.VATperiodStartDate</query>
	<query name="list.currency">from com.vimukti.accounter.core.Currency c order by
		c.name</query>
	<query name="list.TAXAdjustment">from com.vimukti.accounter.core.TAXAdjustment
	</query>

	<query name="list.TAXItemGroup"> from com.vimukti.accounter.core.TAXItemGroup vig
		order by vig.name</query>

	<query name="list.Box">from com.vimukti.accounter.core.Box</query>
	<query name="list.VATReturnBox">from com.vimukti.accounter.core.VATReturnBox vrb
		order by vrb.name
	</query>
	<query name="list.BrandingTheme">from com.vimukti.accounter.core.BrandingTheme bt
		order by bt.id</query>
	<query name="list.locations">from com.vimukti.accounter.core.Location bt
		order by
		bt.id
	</query>
	<query name="unique.name.Account">from com.vimukti.accounter.core.Account a where
		a.name=?</query>
	<query name="unique.name.Item">from com.vimukti.accounter.core.Item a where
		a.name=?
	</query>
	<query name="unique.name.TAXCode">from com.vimukti.accounter.core.TAXCode a where
		a.name=?</query>
	<query name="unique.name.TAXItem">from com.vimukti.accounter.core.TAXItem a where
		a.name=?</query>
	<query name="unique.name.Vendor">from com.vimukti.accounter.core.Vendor a where
		a.name=?</query>
	<query name="unique.name.TAXAgency">from com.vimukti.accounter.core.TAXAgency a where
		a.name=?</query>
	<query name="unique.name.PaymentTerms">from com.vimukti.accounter.core.PaymentTerms a
		where
		a.name=?</query>

	<query name="unique.name.VATReturnBox">from com.vimukti.accounter.core.VATReturnBox v
		where
		v.name=?</query>
	<query name="unique.emailId.User">from com.vimukti.accounter.core.User u where
		u.email=?</query>
	<query name="gettoken.by.id">from com.vimukti.accounter.core.ResetPasswordToken r
		where r.token=?</query>
	<query name="gettokens.by.userid">from com.vimukti.accounter.core.ResetPasswordToken r
		where r.userId=?</query>
</hibernate-mapping>