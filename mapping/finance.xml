<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.vimukti.accounter.core"
	default-access="field" auto-import="false">
	<joined-subclass name="com.vimukti.accounter.workspace.tool.FinanceTool"
		table="FINANCETOOL" extends="com.bizantra.server.workspace.ext.AbstractTool">
		<key column="ID"></key>

		<many-to-one name="company" class="Company" column="COMPANY_ID"
			cascade="save-update,delete">
		</many-to-one>
		<many-to-one name="bizantraCompany"
			class="com.bizantra.server.internal.core.BizantraCompany" column="BIZANTRA_COMPANY_ID"
			cascade="save-update,delete">
		</many-to-one>

	</joined-subclass>




</hibernate-mapping>