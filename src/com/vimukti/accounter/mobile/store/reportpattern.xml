<?xml version="1.0" encoding="utf-8"?>
<patterns>
	<pattern>
		<input>Report</input>
		<input>Reports</input>
		<output>
			<text>"Report is a formatted and organized presentation of your
				company data.</text>
			<command value="Company  &amp; Financial Reports">Company &amp; Financial Reports</command>
			<command value="Customers and Receivables Reports">Customers and Receivables Reports</command>
			<command value="Sales Reports">Sales Reports</command>
			<command value="Suppliers and Payables Reports">Suppliers and Payable Reports</command>
			<command value="Purchases Reports">Purchases Reports</command>
			<command value="VAT Reports">VAT Reports</command>
		</output>
	</pattern>
	<pattern>
		<input>Company &amp; Financial Reports</input>
		<output>
			<text>Company &amp; Financial Reports</text>
			<command>Profit and Loss</command>
			<command>Balance Sheet</command>
			<command>Cash Flow Report</command>
			<command>Trial Balance</command>
			<command>Transaction Detail By Account</command>
			<command>Expense Report</command>
		</output>
	</pattern>
	<pattern>
		<input>Customers and Receivable</input>
		<input>customer reports</input>
		<output>
			<text>Sales Reports</text>
			<command>A/R Aging Summary</command>
			<command>A/R Aging Detail</command>
			<command>Customer Statement</command>
			<command>Customer Transaction History</command>
		</output>
	</pattern>
	<pattern>
		<input>Sales Reports</input>
		<output>
			<text>Sales Reports</text>
			<command>Sales By Customer Summary</command>
			<command>Sales By Customer Detail</command>
			<command>Sales By Item Summary</command>
			<command>Sales By Item Detail</command>
			<command>Sales Order Report</command>
		</output>
	</pattern>
	<pattern>
		<input>Suppliers and Payables Reports</input>
		<input>Vendors and Payables Reports</input>
		<input>vendor reports</input>
		<input>supplier reports</input>
		<output>
			<text>Suppliers and Payables Reports</text>
			<command>A/P Aging Summary</command>
			<command>A/P Aging Detail</command>
			<command>Supplier Transaction History</command>
		</output>
	</pattern>
	<pattern>
		<input>Purchases Reports</input>
		<output>
			<text>Purchases Reports</text>
			<command>Purchase By Supplier Summary</command>
			<command>Purchase By Supplier Detail</command>
			<command>Purchase By Item Summary</command>
			<command>Purchase By Item Detail</command>
			<command>Purchase Order Report</command>
		</output>
	</pattern>
	<pattern>
		<input>VAT Reports</input>
		<input>TAX Reports</input>
		<output>
			<text>VAT Reports</text>
			<command>Prior VAT Return</command>
			<command>VAT Detail</command>
			<command>VAT 100</command>
			<command>Uncategorized VAT Amounts</command>
			<command>VAT Item Summary</command>
			<command>EC Sales List</command>
		</output>
	</pattern>
	<pattern>
		<input>dateRanges</input>
		<output>
			<text>Select A Date Range</text>
			<command value="All">All</command>
			<command value="This Week">This Week</command>
			<command value="This Month">This Month</command>
			<command value="Last Week">Last Week</command>
			<command value="Last Month">Last Month</command>
			<command value="This Financial Year">This Financial Year</command>
			<command value="Last Financial Year">Last Financial Year</command>
			<command value="This Financial Quarter">This Financial Quarter</command>
			<command value="Last Financial Quarter">Last Financial Quarter</command>
			<command value="Financial Year To Date">Financial Year To Date</command>
			<command value="Custom">Custom</command>
		</output>
	</pattern>
	<pattern>
		<input>status</input>
		<output>
			<text>Select A Status</text>
			<command value="Open">Open</command>
			<command value="Completed">Completed</command>
			<command value="Canceled">Canceled</command>
			<command value="All">All</command>
		</output>
	</pattern>
	<pattern>
		<input>expenceType</input>
		<output>
			<text>Select An Expense Type</text>
			<command value="All Expenses">All Expenses</command>
			<command value="Cash">Cash Expense</command>
			<command value="Credit Card">Credit Card Expenses</command>
		</output>
	</pattern>
</patterns>