<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		
		.helpPanel {
			
		}
		
		.helpHPanel {
			background: none repeat scroll 0 0 #1A5158;
		}
		
		.helpLabel {
			
		}
		
		.helpFrame {
			
		}
		
		.helpButton {
			
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="verticalPannel" styleName="{style.helpPanel}">
			<g:Cell width="">
				<g:HorizontalPanel styleName="{style.helpHPanel}"
				ui:field="horizontalhelpPanel" width="100%">
					<g:Label styleName="{style.helpLabel}" ui:field="helpLabel"></g:Label>
				<g:Cell horizontalAlignment="ALIGN_RIGHT">
					<g:ToggleButton ui:field="button" styleName="{style.helpButton}" >
					<g:upFace>
						<img src="../images/helpdialog.jpg" />
					</g:upFace>
					<g:downFace>
						<img src="../images/helppanel.jpg" />
					</g:downFace>
				</g:ToggleButton>
				</g:Cell>
			</g:HorizontalPanel>
			</g:Cell>
			<g:Frame ui:field="frame" styleName="{style.helpFrame}"></g:Frame>
		</g:VerticalPanel>
		<!-- <g:Button styleName="{style.important}" ui:field="button" /> -->
	</g:HTMLPanel>
</ui:UiBinder> 