<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
		<g:VerticalPanel spacing="6">
			<g:VerticalPanel spacing="6">
				<g:HorizontalPanel spacing="4">
					<g:CheckBox ui:field="classTrackingCheckBox"/>
					<g:Label ui:field="classTrackingCheckBoxLabel"/>
				</g:HorizontalPanel>
			</g:VerticalPanel>
			<g:VerticalPanel spacing="6" ui:field="hiddenPanel">
				<g:HorizontalPanel spacing="6">
					<g:CheckBox ui:field="classWarningCheckBox"/>
					<g:Label ui:field="classWarningCheckBoxLabel" horizontalAlignment="ALIGN_LEFT"/>
				</g:HorizontalPanel>
				<g:Label ui:field="classesOnSalesLabel"/>
				<g:HorizontalPanel spacing="6">
					<g:RadioButton name="new name" ui:field="onePerTransactionRadio"/>
				</g:HorizontalPanel>
				<g:HorizontalPanel spacing="6">
					<g:RadioButton name="new name" ui:field="onePerDetailLineRadio"/>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
</ui:UiBinder> 