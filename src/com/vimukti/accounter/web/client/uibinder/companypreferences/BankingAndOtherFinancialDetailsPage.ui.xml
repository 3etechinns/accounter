<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="8">
		<g:VerticalPanel styleName="companyInfoPanel">
			<g:Label ui:field="bankingLabelItem" styleName="header"/>
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:Label ui:field="bankNameTextLabel"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="bankNameTextBox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="bankAccountNoTextLabel"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="bankAccountNoTextBox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="sortCodeTextLabel"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="sortCodeTextBox"/>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:VerticalPanel>
		<g:VerticalPanel styleName="companyInfoPanel">
			<g:Label ui:field="otherFinancialLabelItem" styleName="header"/>
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:Label ui:field="companyRegistrationNoTextLabel"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="companyRegistrationNoTextBox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="vatRegistartionNoTextLabel"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="vatRegistartionNoTextBox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="federalTaxIDTextLabel"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="federalTaxIDTextBox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="primaryCurrencyComboLabel"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox visibleItemCount="1" ui:field="primaryCurrencyCombo"/>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:VerticalPanel></g:VerticalPanel>
</ui:UiBinder> 