<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:VerticalPanel spacing="6">
			<g:HorizontalPanel spacing="10">
				<g:Label horizontalAlignment="ALIGN_LEFT" ui:field="contactDetailLabel"/>
			</g:HorizontalPanel>
			<g:VerticalPanel spacing="4">
				<g:Grid>
					<g:row>
						<g:customCell>
							<g:Label ui:field="phoneTextBoxLabel"/>
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="phoneTextBox"/>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="emailFieldBoxLabel"/>
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="emailFieldBox"/>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="faxTextBoxLabel"/>
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="faxTextBox"/>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="webTextBoxLabel"/>
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="webTextBox"/>
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 