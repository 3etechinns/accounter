<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.skip_start {
			padding: 4px;
		}
		
		.skip_start TR TD {
			padding: 4px;
		}
		
		.skip_start TR TD BUTTON {
			float: right !important;
		}
	</ui:style>
	<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:VerticalPanel ui:field="viewPanel">
			<g:HTML ui:field="setupInfo" wordWrap="true" styleName="setup_Info"></g:HTML>
			<g:HorizontalPanel>
				<g:Grid styleName="skip_setup">
					<g:row>
						<g:customCell>
							<g:HTML ui:field="skipSetupInfo" wordWrap="true"></g:HTML>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Button ui:field="skipButton"></g:Button>
						</g:customCell>
					</g:row>
				</g:Grid>
				<g:Grid styleName="start_setup">
					<g:row>
						<g:customCell>
							<g:HTML ui:field="startSetupInfo" wordWrap="true"></g:HTML>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Button ui:field="startButton"></g:Button>
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:HorizontalPanel>
			<g:Button ui:field="cancelBtn" ></g:Button>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 