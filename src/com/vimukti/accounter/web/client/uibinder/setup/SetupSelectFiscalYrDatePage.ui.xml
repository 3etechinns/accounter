<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:VerticalPanel ui:field="viewPanel">
			<g:HTML wordWrap="true" ui:field="fiscalHead"/>
			<g:HTML wordWrap="true" ui:field="fiscalInfo"/>
			<g:HorizontalPanel>
				<g:Label ui:field="fiscalStarts"/>
				<g:ListBox ui:field="fiscalStartsList"/>
			</g:HorizontalPanel>
			<g:VerticalPanel>
				<g:HTML wordWrap="true" ui:field="fiscalHeader"/>
				<g:Label ui:field="startDate"/>
				<g:RadioButton name="new name" ui:field="beginingFiscal"/>
				<g:HTML wordWrap="true" ui:field="beginingFiscalText"/>
				<g:RadioButton name="new name" ui:field="todaysDate"/>
				<g:HTML wordWrap="true" ui:field="todaysDateText"/>
				<p1:DateBox ui:field="fiscalDate"/>
			</g:VerticalPanel>
		</g:VerticalPanel>
		</g:VerticalPanel>
</ui:UiBinder> 