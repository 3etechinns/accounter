<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
		<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:Label ui:field="customerCommentLabel" styleName="refer_customer"/>
		<g:HorizontalPanel styleName="refer_panel">
			<g:Label ui:field="customerLabel"/>
			<g:ListBox ui:field="customerListBox" styleName="refer_listbox"/>
		</g:HorizontalPanel>
		<g:Label ui:field="supplierCommentLabel" styleName="refer_supplier"/>
		<g:HorizontalPanel styleName="refer_panel">
			<g:Label ui:field="vendorsLabel"/>
			<g:ListBox ui:field="vendorListBox" styleName="refer_listbox"/>
		</g:HorizontalPanel>
		<g:Label ui:field="accountCommentLabel" styleName="refer_account"/>
		<g:HorizontalPanel styleName="refer_panel">
			<g:Label ui:field="accountLabel"/>
			<g:ListBox ui:field="accountListBox" styleName="refer_listbox"/>
		</g:HorizontalPanel>
		</g:VerticalPanel>
</ui:UiBinder> 