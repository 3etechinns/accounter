<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
		<g:VerticalPanel width="353px" height="375px" ui:field="viewPanel">
			<g:Label ui:field="headerLabel"/>
			<g:Grid height="199px" ui:field="referPanel">
				<g:row>
					<g:customCell>
						<g:Label width="304px" ui:field="customerLabel"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="customerListBox"/>
					</g:customCell>
				</g:row>
						<g:row>
					<g:customCell>
						<g:Label ui:field="customerCommentLabel"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="supplierLabel"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="supplierListBox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="supplierCommentLabel"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="accountLabel"/>
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="accountListBox"/>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="accountCommentLabel"/>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:VerticalPanel>
</ui:UiBinder> 