<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel">
			<g:Label ui:field="AccounterInfo"></g:Label>
			<g:Grid ui:field="companyInfoField" styleName="company_Info_table">
				<g:row>
					<g:customCell>
						<g:Label width="" height="" ui:field="displayNameLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="companyName" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label height="" width="" ui:field="legalNameLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="legalName" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label />
					</g:customCell>
					<g:customCell>
						<g:Label wordWrap="true" ui:field="LegalForm"></g:Label>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="taxIDLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="taxId" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label />
					</g:customCell>
					<g:customCell>
						<g:Label wordWrap="true" ui:field="EIN"></g:Label>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="countryLabel" />
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="country" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="stateLabel" />
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="stateListBox" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="streetAdreess1Label" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="streetAddress1" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="streetAddress2Label" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="streetAdress2" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="cityLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="cityTextBox" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="zipLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="zip" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="phoneLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="phone" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="faxLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="fax" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label />
					</g:customCell>
					<g:customCell>
						<g:Label wordWrap="true" ui:field="useFormat"></g:Label>
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="emailAdressLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="emailAddress" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="webSiteLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="webSite" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="timezone" />
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="timezoneslistbox" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 