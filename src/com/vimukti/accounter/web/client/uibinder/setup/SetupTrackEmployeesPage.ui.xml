<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:VerticalPanel ui:field="viewPanel">
			<g:RadioButton name="trackEmployee" ui:field="trackEmployeeYes" />
			<g:VerticalPanel>
				<g:CheckBox ui:field="w2Employees" />
				<g:CheckBox ui:field="contractors" />
			</g:VerticalPanel>
			<g:RadioButton name="trackEmployee" ui:field="trackEmployeeNo" />
		</g:VerticalPanel>
		<g:VerticalPanel ui:field="trackPanel">
			<g:Label wordWrap="true" ui:field="trackExpenses" styleName="setup_header_label" />
			<g:RadioButton name="trackEmployeeType" ui:field="trackEmployeeExpenseYes" />
			<g:RadioButton name="trackEmployeeType" ui:field="trackEmployeeExpenseNo" />
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 