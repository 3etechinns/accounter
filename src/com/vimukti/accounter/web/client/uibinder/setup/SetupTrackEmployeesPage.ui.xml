<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel">
			<g:RadioButton name="trackEmployee" ui:field="trackEmployeeYes"
				styleName="employee_yesno" />
			<g:FlowPanel styleName="check_label">
				<g:CheckBox ui:field="w2Employees" />
				<g:CheckBox ui:field="contractors" />
			</g:FlowPanel>
			<g:RadioButton name="trackEmployee" ui:field="trackEmployeeNo"
				styleName="employee_yesno" />
		</g:FlowPanel>
		<g:FlowPanel ui:field="trackPanel" styleName="track_emp_vpanel">
			<g:Label wordWrap="true" ui:field="trackExpenses" styleName="setup_header_label" />
			<g:RadioButton name="trackEmployeeType" ui:field="trackEmployeeExpenseYes"
				styleName="employee_yesno" />
			<g:RadioButton name="trackEmployeeType" ui:field="trackEmployeeExpenseNo"
				styleName="employee_yesno" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 