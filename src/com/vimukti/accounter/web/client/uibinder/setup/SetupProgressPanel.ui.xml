<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:VerticalPanel ui:field="viewPanel">
			<g:HorizontalPanel>
				<g:Image ui:field="setupImage"/>
				<g:Label ui:field="startUp"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="companyInfoImage"/>
				<g:Label ui:field="companyInformation"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="industryTypeImage"/>
				<g:Label ui:field="industryType"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="organizationImage"/>
				<g:Label ui:field="organization"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="referringImage"/>
				<g:Label ui:field="refferingName"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="trackImage"/>
				<g:Label ui:field="trackEmployee"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="sellImage"/>
				<g:Label ui:field="sell"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="estimateImage"/>
