			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="currencyImage"/>
				<g:Label ui:field="currency"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="billTrackingImage"/>
				<g:Label ui:field="billTracking"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="fiscalYearImage"/>
				<g:Label ui:field="fiscalYear"/>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Image ui:field="requiredImage"/>
				<g:Label ui:field="requiredAccounts"/>
			</g:HorizontalPanel>
		</g:VerticalPanel>
		</g:VerticalPanel>
</ui:UiBinder> 