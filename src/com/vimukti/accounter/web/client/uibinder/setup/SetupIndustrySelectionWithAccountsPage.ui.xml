<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:VerticalPanel ui:field="viewPanel" width="" height="">
			<g:HTML ui:field="customizeAccounter">
			</g:HTML>
			<g:HTML ui:field="settingsLater">
			</g:HTML>
			<g:HTML ui:field="industry" styleName="industry">
			</g:HTML>
			<g:HorizontalPanel ui:field="industryAccountsPanel">
				<g:VerticalPanel>
					<g:HTML ui:field="selectIndustry" styleName="select_industry" />
					<g:ListBox ui:field="industryList" styleName="industry_list" />
				</g:VerticalPanel>
				<g:VerticalPanel>
					<g:HTML ui:field="accountsLabel"/>
					<g:ScrollPanel height="250px" width="250px">
					<g:VerticalPanel height="300px" ui:field="accountsPanel"/>
					</g:ScrollPanel> 
				</g:VerticalPanel>
			</g:HorizontalPanel>
			
		</g:VerticalPanel>
</g:VerticalPanel>
</ui:UiBinder> 