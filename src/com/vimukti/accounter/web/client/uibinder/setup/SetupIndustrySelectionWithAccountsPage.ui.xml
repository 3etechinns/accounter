<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel" width="100%" height="100%">
			<g:Label ui:field="customizeAccounter" styleName="customize_Accounter_labels">
			</g:Label>
			<g:FlowPanel ui:field="industryAccountsPanel">
				<g:FlowPanel styleName="industry_list_table">
					<g:Label ui:field="selectIndustryhead" styleName="select_industry" />
					<g:ListBox ui:field="industryList" visibleItemCount="15"
						styleName="industry_list" />
				</g:FlowPanel>
				<g:FlowPanel styleName="sub_industry_listpanel">
					<g:Label ui:field="accountsLabel" styleName="accounts_label" />
					<g:ScrollPanel height="230px" width="250px">
						<g:FlowPanel ui:field="accountsPanel" />
					</g:ScrollPanel>
				</g:FlowPanel>
			</g:FlowPanel>

		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 