<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel" width="" height="">
			<g:CheckBox ui:field="enableBox" />
			<g:Label wordWrap="true" ui:field="encryptionInfo" />
			<g:Grid ui:field="companyInfoField"
				styleName="company_Info_table">
				<g:row>
					<g:customCell>
						<g:Label ui:field="passwordLabel" />
					</g:customCell>
					<g:customCell>
						<g:PasswordTextBox ui:field="passwordField" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label ui:field="conformPasswordLabel" />
					</g:customCell>
					<g:customCell>
						<g:PasswordTextBox ui:field="conformPasswordField" />
					</g:customCell>
				</g:row>
				
				<g:row>
					<g:customCell>
						<g:Label ui:field="hintLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox  ui:field="hintField" />
					</g:customCell>
				</g:row>
			</g:Grid>
			<g:Label wordWrap="true" ui:field="passwordInfo" />
			<g:Hyperlink ui:field="moreInfoLink" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 