<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		 
			
		}
	</ui:style>
	<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:VerticalPanel ui:field="viewPanel" width="" height="">
		 <g:HorizontalPanel><g:CheckBox ui:field="enableBox" /><g:Label ui:field="enableEncryption" /></g:HorizontalPanel>
		<g:HTML wordWrap="true" ui:field="encryptionInfo" />
		<g:HorizontalPanel>
		<g:Label ui:field="passwordLabel" />
		<g:PasswordTextBox  ui:field="passwordField" /></g:HorizontalPanel>
		<g:HorizontalPanel>
		<g:Label ui:field="conformPasswordLabel" />
		<g:PasswordTextBox  ui:field="conformPasswordField" /></g:HorizontalPanel>
		
		<g:HTML wordWrap="true" ui:field="passwordInfo" />
