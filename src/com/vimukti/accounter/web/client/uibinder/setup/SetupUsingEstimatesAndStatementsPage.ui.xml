<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:VerticalPanel ui:field="viewPanel">
			<g:RadioButton name="estimateGroup" ui:field="estimatesYes" />
			<g:RadioButton name="estimateGroup" ui:field="estimatesNo" />
			<g:VerticalPanel>
				<g:Label ui:field="billingStatements" />
				<g:Label ui:field="someExampleText" />
				<g:HTML wordWrap="true" ui:field="someExampleList" />
				<g:VerticalPanel>
					<g:Label ui:field="billingQuestion" />
					<g:RadioButton name="statementGroup" ui:field="statementYes" />
					<g:RadioButton name="statementGroup" ui:field="statementsNo" />
				</g:VerticalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 