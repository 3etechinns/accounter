<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="viewPanel">
			<g:RadioButton name="new name" ui:field="estimatesYes"/>
			<g:RadioButton name="new name" ui:field="estimatesNo"/>
			<g:VerticalPanel>
				<g:Label ui:field="billingStatements"/>
				<g:Label ui:field="someExampleText"/>
				<g:HTML wordWrap="true" ui:field="someExampleList"/>
				<g:VerticalPanel>
					<g:Label ui:field="billingQuestion"/>
					<g:RadioButton name="new name" ui:field="statementYes"/>
					<g:RadioButton name="new name" ui:field="statementsNo"/>
				</g:VerticalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>

	</g:HTMLPanel>
</ui:UiBinder> 