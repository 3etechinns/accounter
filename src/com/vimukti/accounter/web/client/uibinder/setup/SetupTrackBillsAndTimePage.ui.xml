<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:VerticalPanel ui:field="viewPanel">
			<g:VerticalPanel ui:field="managing">
				<g:HTML wordWrap="true" ui:field="managingInfo" />
				<g:HTML wordWrap="true" ui:field="managingList" />
				<g:HTML wordWrap="true" ui:field="trackTimeText" />
				<g:RadioButton name="new name" ui:field="managingYes" />
				<g:RadioButton name="new name" ui:field="managingNo" />
			</g:VerticalPanel>
			<g:VerticalPanel ui:field="trackingBillsPanel">
				<g:HTML wordWrap="true" ui:field="trackingTimeDes" />
				<g:HTML wordWrap="true" ui:field="trackOfBillsList" />
				<g:HTML wordWrap="true" ui:field="trackOfBillsText" />
				<g:RadioButton name="new name" ui:field="trackingTimeYes" />
				<g:RadioButton name="new name" ui:field="trackingNo" />
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 