<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
		
		.managing-html {
			margin-left: 30px;
		}
	</ui:style>
	<g:FlowPanel ui:field="mainViewPanel">
		<g:Label ui:field="headerLabel" styleName="setup_header_label" />
		<g:FlowPanel ui:field="viewPanel">
			<g:FlowPanel ui:field="managing" styleName="bills_panel">
				<g:Label wordWrap="true" ui:field="managingInfo" />
				<g:Label styleName="managing-html" wordWrap="true" ui:field="managingList" />
				<g:Label wordWrap="true" ui:field="trackTimeText" styleName="track_question" />
				<g:RadioButton name="bills" ui:field="managingYes"
					styleName="employee_yesno" />
				<g:RadioButton name="bills" ui:field="managingNo"
					styleName="employee_yesno" />
			</g:FlowPanel>
			<g:FlowPanel ui:field="trackingBillsPanel" styleName="track_time_panel">
				<g:Label styleName="setup_header_label" ui:field="track_time_head" />
				<g:Label wordWrap="true" ui:field="trackingTimeDes" />
				<g:Label wordWrap="true" ui:field="trackOfBillsList" />
				<g:Label wordWrap="true" ui:field="trackOfBillsText"
					styleName="track_question" />
				<!-- <g:RadioButton name="time" ui:field="trackingTimeYes" styleName="employee_yesno" 
					/> <g:RadioButton name="time" ui:field="trackingNo" styleName="employee_yesno" 
					/> -->
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 