<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:VerticalPanel ui:field="radioButtonsMainPanel"
			spacing="6">
			<g:HorizontalPanel ui:field="headerLabelPanel"
				spacing="6" horizontalAlignment="ALIGN_RIGHT">
				<g:Label ui:field="radioButtonHeaderLabel" />
			</g:HorizontalPanel>
			<g:VerticalPanel spacing="6" ui:field="radioButtonsPanel">
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="oneOwnerRadioButton" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="twoOrmoreownersRadioButton"
						checked="true" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name"
						ui:field="oneormoreshareholdersRadioButton" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="nonprofitRadioButton" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="otherAndNoneRadioButton" />
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 