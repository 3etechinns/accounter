<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="5" styleName="companyInfoPanel">
		<g:Label ui:field="trackEstimateHeader" styleName="header"/>
		<g:Label ui:field="trackEstimateDescLabel"/>
		<g:HorizontalPanel>
			<g:RadioButton name="trackEstimates" ui:field="yesRadioButton"/>
			<g:RadioButton name="trackEstimates" ui:field="noRadioButton"/>
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 