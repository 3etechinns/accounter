<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="8" styleName="companyInfoPanel">
		<g:VerticalPanel ui:field="negativeMainPanel">
			<g:Label styleName="header" ui:field="negativeNoheaderLabel" />
			<g:HorizontalPanel>
				<g:VerticalPanel>
					<g:RadioButton name="negativeNumberFormat" ui:field="normalFormatRadioButton" />
					<g:RadioButton name="negativeNumberFormat"
						ui:field="withInParathesisFormatRadioButton" />
					<g:RadioButton name="negativeNumberFormat"
						ui:field="minusWithInParanthesisRadioButton" />
					<g:RadioButton name="negativeNumberFormat" ui:field="withInTrailingMinusRadioButton" />
				</g:VerticalPanel>
				<g:VerticalPanel>
					<g:Label ui:field="normalFormatCommentLabel" styleName="account_comment" />
					<g:Label ui:field="withInParathesisFormatCommentLabel"
						styleName="account_comment" />
					<g:Label ui:field="minusWithInParanthesisCommentLabel"
						styleName="account_comment" />
					<g:Label ui:field="withInTrailingMinusCommentLabel"
						styleName="account_comment" />
				</g:VerticalPanel>
			</g:HorizontalPanel>
		</g:VerticalPanel>
		<g:VerticalPanel ui:field="decimalMainPanel">
			<g:Label ui:field="decimalHeaderLabel" styleName="header" />
			<g:HorizontalPanel>
				<g:Label ui:field="comboLabel" />
				<g:ListBox visibleItemCount="1" ui:field="decimalDigitLimitCombo"
					width="80px" />
			</g:HorizontalPanel>
			<g:Label ui:field="commentLabel" styleName="account_comment"
				width="343" />
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 