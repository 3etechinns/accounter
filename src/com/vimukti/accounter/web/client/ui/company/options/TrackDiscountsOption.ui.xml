<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="5" styleName="companyInfoPanel">
		<g:CheckBox ui:field="trackCheckbox" styleName="header" />
		<g:Label ui:field="selectDiscount" />
		<g:VerticalPanel spacing="5" ui:field="hidePanel">
			<g:Label ui:field="selectDiscountAccount" />
			<g:HorizontalPanel spacing="5" ui:field="accountPanel">
				<g:Label ui:field="discountAccount" />
				<g:ListBox ui:field="accountsCombo" />
			</g:HorizontalPanel>
		</g:VerticalPanel>

	</g:VerticalPanel>
</ui:UiBinder> 