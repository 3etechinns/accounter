<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="8" styleName="companyInfoPanel">
		<g:VerticalPanel horizontalAlignment="ALIGN_CENTER" width="436" height="186">
			<g:Grid cellPadding="4" cellSpacing="8" width="195" height="56">
				<g:row>
					<g:customCell>
						<g:Label ui:field="companyWebsiteHeaderLabel" styleName="header"/>
					</g:customCell>
					<g:customCell>
						<g:TextBox visibleLength="35" ui:field="companyWebsiteTextBox"/>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 