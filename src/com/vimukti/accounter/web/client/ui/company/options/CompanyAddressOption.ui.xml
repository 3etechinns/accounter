<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="8" styleName="companyInfoPanel">
		<g:VerticalPanel ui:field="tradingAddressPanel">
			<g:Label ui:field="tradingAddressTitle" horizontalAlignment="ALIGN_LEFT"
				styleName="header" />
			<g:Label ui:field="tradingAddressDescription" />
			<g:VerticalPanel horizontalAlignment="ALIGN_CENTER"
				ui:field="tradingAddressSubPanel">
				<g:Grid>
					<g:row>
						<g:customCell>
							<g:Label ui:field="tAddress1Label" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="tAddress1TextBox" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="tAddress2Label" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="tAddress2TextBox" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="tCityLabel" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="tCityTextBox" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="tPostalCodeLabel" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="tPostalCodeTextBox" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="tCountryLabel" />
						</g:customCell>
						<g:customCell>
							<g:ListBox width="185px" ui:field="tCountryCombo" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="tStateLabel" />
						</g:customCell>
						<g:customCell>
							<g:ListBox width="185px" ui:field="tStateCombo" enabled="false" />
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:VerticalPanel>
		</g:VerticalPanel>

		<g:HorizontalPanel spacing="5">
			<g:CheckBox ui:field="isShowRegisteredAddressCheckBox" />
		</g:HorizontalPanel>
		<g:VerticalPanel ui:field="registeredAddressPanel" spacing="5">
			<g:Label ui:field="registeredAddressTitle"
				horizontalAlignment="ALIGN_LEFT" styleName="header" />
			<g:Label ui:field="registeredAddressDescription" />

			<g:VerticalPanel spacing="5" ui:field="registeredAddressSubPanel"
				horizontalAlignment="ALIGN_CENTER">
				<g:Grid>
					<g:row>
						<g:customCell>
							<g:Label ui:field="rAddress1Label" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="rAddress1TextBox" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="rAddress2Label" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="rAddress2TextBox" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="rCityLabel" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="rCityTextBox" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="rPostalCodeLabel" />
						</g:customCell>
						<g:customCell>
							<g:TextBox ui:field="rPostalCodeTextbox" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="rCountryComboLabel" />
						</g:customCell>
						<g:customCell>
							<g:ListBox width="185px" ui:field="rCountryCombo" />
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:Label ui:field="rStateComboLabel" />
						</g:customCell>
						<g:customCell>
							<g:ListBox width="185px" ui:field="rStateCombo" enabled="false" />
						</g:customCell>
					</g:row>
				</g:Grid>
			</g:VerticalPanel>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 