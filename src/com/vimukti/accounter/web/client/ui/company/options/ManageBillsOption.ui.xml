<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel styleName="companyInfoPanel" horizontalAlignment="ALIGN_LEFT" spacing="6">
		<g:Label ui:field="managingBillLabelItem" styleName="header" />
		<g:HorizontalPanel horizontalAlignment="ALIGN_CENTER" spacing="6">
			<g:RadioButton name="managingBillRadioButton" ui:field="managingBillYesRadioButton" />
			<g:RadioButton name="managingBillRadioButton" ui:field="managingBillNoRadioButton"
				checked="true" />
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 