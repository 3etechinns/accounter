<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:VerticalPanel ui:field="radioButtonsMainPanel"
			spacing="6" horizontalAlignment="ALIGN_LEFT" styleName="companyInfoPanel">
			<g:HorizontalPanel ui:field="headerLabelPanel"
				spacing="6">
				<g:Label ui:field="radioButtonHeaderLabel" styleName="header" />
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:VerticalPanel ui:field="viewPanel">
					<g:VerticalPanel ui:field="radioButtonPanel" spacing="1">
				<g:Label ui:field="organizeText" />
				<g:RadioButton name="new name" ui:field="propriterShip" />
				<g:Label ui:field="uninCorporated" />
				<g:RadioButton name="new name" ui:field="partnership" />
				<g:Label ui:field="morePartners" />
				<g:RadioButton name="new name" ui:field="lLC" />
				<g:Label ui:field="lLCText" />
				<g:RadioButton name="new name" ui:field="corporation" />
				<g:Label ui:field="corporationText" />
				<g:RadioButton name="new name" ui:field="sCorporation" />
				<g:Label ui:field="sCorporationText" />
				<g:RadioButton name="new name" ui:field="nonProfit" />
				<g:Label ui:field="nonProfitText" />
				<g:RadioButton name="new name" ui:field="other" />
			</g:VerticalPanel>
				</g:VerticalPanel>
				<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE">
					<g:ListBox ui:field="lLCCombo" />
				</g:Cell>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 