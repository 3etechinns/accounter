<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="8" styleName="companyInfoPanel"
		horizontalAlignment="ALIGN_CENTER">
		<g:FlowPanel  ui:field="hpanel">
			<g:CheckBox ui:field="locationTrackingCheckBoxItm" width="" />
			<g:Label ui:field="locationTrackingCheckBoxLabel" styleName="header" />
		</g:FlowPanel>
		<g:HorizontalPanel>
			<g:Label ui:field="locationTrackingDescriptionLabel" />
		</g:HorizontalPanel>
		<g:FlowPanel  ui:field="radioButtonsPanel">
			<g:HorizontalPanel>
				<g:Label ui:field="radioButtonsHeaderLabel" styleName="header" />
			</g:HorizontalPanel>
			<g:FlowPanel ui:field="radioButtonPanel">
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="buisinessRadioButton" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="locationRadioButton" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="departmentRadioButton" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="divisionRadioButton" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="propertyRadioButton" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="storeRadioButton" />
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:RadioButton name="new name" ui:field="territoryRadioButton" />
				</g:HorizontalPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:VerticalPanel>
</ui:UiBinder> 