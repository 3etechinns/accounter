<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:FlowPanel styleName="companyInfoPanel">
		<g:FlowPanel styleName="currencyAndListBoxPanel">
			<g:Label ui:field="primaryCurrenyLabel" styleName="header" />
			<g:ListBox visibleItemCount="1" ui:field="primaryCurrencyListBox" />
		</g:FlowPanel>
		<g:CheckBox ui:field="isEnableMultiCurrencyCheckBox" />
		<g:Label ui:field="currencyCommentLabel" styleName="organisation_comment" />
	</g:FlowPanel>
</ui:UiBinder> 