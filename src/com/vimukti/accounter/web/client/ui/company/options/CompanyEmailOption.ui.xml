<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="8" styleName="companyInfoPanel">
		<g:VerticalPanel horizontalAlignment="ALIGN_CENTER">
			<g:Grid cellPadding="5" cellSpacing="10">
				<g:row>
					<g:customCell>
						<g:Label ui:field="companyEmailHeaderLabel" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="companyEmailTextBox" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:VerticalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field="emailIDDescriptionLabel" />
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 