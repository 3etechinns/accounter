<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="8" styleName="companyInfoPanel">
		<g:HorizontalPanel spacing="10">
			<g:Label ui:field="companyEmailHeaderLabel" styleName="header" />
			<g:TextBox visibleLength="35" ui:field="companyEmailTextBox" />
		</g:HorizontalPanel>
		<g:HorizontalPanel>
			<g:Label ui:field="emailIDDescriptionLabel" />
		</g:HorizontalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 