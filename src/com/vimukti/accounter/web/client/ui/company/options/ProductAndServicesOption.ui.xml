<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:VerticalPanel spacing="5" styleName="companyInfoPanel">
		<g:Label ui:field="headerLabel" styleName="header" />
		<g:FlowPanel ui:field="viewPanel">
			<g:FlowPanel ui:field="sell" styleName="sell_panel">
				<g:RadioButton name="sell_type" ui:field="servicesOnly" />
				<g:Label ui:field="servicesOnlyText" styleName="organisation_comment" />
				<g:RadioButton name="sell_type" ui:field="productsOnly" />
				<g:Label ui:field="productsOnlyText" styleName="organisation_comment" />
				<g:RadioButton name="sell_type" ui:field="both" />
				<g:Label ui:field="bothText" styleName="organisation_comment" />
			</g:FlowPanel>
		</g:FlowPanel>
		<g:FlowPanel ui:field="totalPanel">
			<g:HorizontalPanel spacing="10">
				<g:CheckBox ui:field="inventoryCheckBox" />
			</g:HorizontalPanel>
			<g:FlowPanel ui:field="hiddenPanel">
				<g:HorizontalPanel horizontalAlignment="ALIGN_LEFT">
					<g:FlowPanel ui:field="subpanel">
						<g:CheckBox ui:field="warehousesCheckBox" />
						<g:CheckBox ui:field="unitsCheckBox" />
						<g:HorizontalPanel>
							<g:Label ui:field="inventoryScheme" />
							<g:ListBox ui:field="inventorySchemeListBox"></g:ListBox>
						</g:HorizontalPanel>
					</g:FlowPanel>
				</g:HorizontalPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:VerticalPanel>
</ui:UiBinder> 