<hibernate-mapping package="com.vimukti.accounter.web.client.core"
	default-access="field">
	<class name="Transaction" table="TRANSACTION">
		<id name="id" column="ID_TRANSACTION">
			<generator class="increment" />
		</id>
		<property name="version" column="VERSION"></property>
		<property name="createdDate" column="CREATED_DATE" type="date"></property>
		<property name="modifiedOn" column="MODIFIED_ON" type="date"></property>
		<property name="key" column="KEY"></property>
		<property name="date" column="T_DATE" type="date"></property>
		<property name="type" column="T_TYPE"></property>
		<many-to-one name="company" class="Company" lazy="false"
			column="COMPANY_ID">
		</many-to-one>
		<property name="number" column="NUMBER"></property>

		<list name="transactionItems" cascade="save-update,delete,delete-orphan"
			lazy="false" >
			<key column="TRANSACTION_ID" />
			<list-index column="INDEX" />
			<one-to-many class="TransactionItem" />
		</list>
		<list name="transactionMakeDeposit" cascade="save-update,delete,delete-orphan"
			lazy="false">
			<key column="TRANSACTION_ID" />
			<list-index column="INDEX" />
			<one-to-many class="TransactionMakeDeposit" />
		</list>
		<list name="transactionPayBill" cascade="save-update,delete,delete-orphan"
			lazy="false">
			<key column="TRANSACTION_ID" />
			<list-index column="INDEX" />
			<one-to-many class="TransactionPayBill" />
		</list>
		<list name="transactionReceivePayment" cascade="save-update,delete,delete-orphan"
			lazy="false">
			<key column="TRANSACTION_ID" />
			<list-index column="INDEX" />
			<one-to-many class="TransactionReceivePayment" />
		</list>
		<list name="transactionIssuePayment" cascade="save-update,delete,delete-orphan"
			lazy="false">
			<key column="TRANSACTION_ID" />
			<list-index column="INDEX" />
			<one-to-many class="TransactionIssuePayment" />
		</list>

		<property name="status" column="STATUS"></property>
		<property name="canVoidOrEdit" column="CAN_VOID_OR_EDIT"></property>
		<list name="entry" cascade="save-update,delete,delete-orphan"
			lazy="false">
			<key column="TRANSACTION_ID" />
			<list-index column="INDEX" />
			<one-to-many class="Entry" />
		</list>

		<property name="isDeposited" column="IS_DEPOSITED"></property>

