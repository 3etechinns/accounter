	<pattern login="true">
		<input>Reports</input>
		<outputs>
			<command title="Company &amp; Financial Reports" />
			<command title="Customers and Receivables Reports" />
			<command title="Sales Reports" />
			<command title="Suppliers and Payable Reports" />
			<command title="Purchases Reports" />
			<command title="VAT Reports" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>Company &amp; Financial Reports</input>
		<outputs>
			<text name="Company &amp; Financial Reports" />
			<command title="Profit and Loss" />
			<command title="Balance Sheet" />
			<command title="Cash Flow Report" />
			<command title="Trial Balance" />
			<command title="Transaction Detail By Account" />
			<command title="Expense Report" />
			<command title="Transaction Detail by Tax Item" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>Customers and Receivables Reports</input>
		<input>customer reports</input>
		<outputs>
			<text name="Sales Reports" />
			<command title="A/R Aging Summary" />
			<command title="A/R Aging Detail" />
			<command title="Customer Statement" />
			<command title="Customer Transaction History" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>Sales Reports</input>
		<outputs>
			<text name="Sales Reports" />
			<command title="Sales By Customer Summary" />
			<command title="Sales By Customer Detail" />
			<command title="Sales By Item Summary" />
			<command title="Sales By Item Detail" />
			<command title="Sales Order Report" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>Suppliers and Payable Reports</input>
		<input>Vendors and Payable Reports</input>
		<input>vendor reports</input>
		<input>supplier reports</input>
		<outputs>
			<text name="Suppliers and Payable Reports" />
			<command title="A/P Aging Summary" />
			<command title="A/P Aging Detail" />
			<command title="Supplier Transaction History" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>Purchases Reports</input>
		<outputs>
			<text name="Purchases Reports" />
			<command title="Purchase By Supplier Summary" />
			<command title="Purchase By Supplier Detail" />
			<command title="Purchase By Item Summary" />
			<command title="Purchase By Item Detail" />
			<command title="Purchase Order Report" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>VAT Reports</input>
		<input>TAX Reports</input>
		<outputs>
			<text name="VAT Reports" />
			<command title="Prior VAT Return" />
			<command title="VAT Detail" />
			<command title="VAT 100" />
			<command title="Uncategorized VAT Amounts" />
			<command title="VAT Item Summary" />
			<command title="EC Sales List" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>dateRanges</input>
		<outputs>
			<text name="Select A Date Range" />
			<command title="All" />
			<command title="This Week" />
			<command title="This Month" />
			<command title="Last Week" />
			<command title="Last Month" />
			<command title="This Financial Year" />
			<command title="Last Financial Year" />
			<command title="This Financial Quarter" />
			<command title="Last Financial Quarter" />
			<command title="Financial Year To Date" />
			<command title="Custom" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>status</input>
		<outputs>
			<text name="Select A Status" />
			<command title="Open" />
			<command title="Completed" />
			<command title="Canceled" />
			<command title="All" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>expenceType</input>
		<outputs>
			<text name="Select An Expense Type" />
			<command title="All Expenses" />
			<command title="Cash Expense" />
			<command title="Credit Card Expenses" />
		</outputs>
	</pattern>
