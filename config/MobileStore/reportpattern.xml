<?xml version="1.0" encoding="utf-8"?>
<patterns>
	<!-- <pattern login="true"> <input>Report</input> <input>Reports</input> 
		<input>How to see the Reports</input> <outputs> <text name=""Report is a 
		formatted and organized presentation of your company data. " /> <command 
		value="Company &amp; Financial Reports">Company &amp; Financial Reports" 
		/> <command value="Customers and Receivables Reports">Customers and Receivables 
		Reports" /> <command value="Sales Reports">Sales Reports" /> <command value="Suppliers 
		and Payables Reports">Suppliers and Payable Reports" /> <command value="Purchases 
		Reports">Purchases Reports" /> <command value="VAT Reports">VAT Reports" 
		/> </outputs> </pattern> -->

	<pattern login="true">
		<input>reports</input>
		<outputs>
			<command title="companyFinancialReports" />
			<command title="{Customers}andReceivablesReports" />
			<command title="salesReports" />
			<command title="purchasesReports" />
			<command if="trackTax" title="vATReports" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>companyFinancialReports</input>
		<outputs>
			<text name="companyFinancialReports" />
			<command title="profitandLoss" />
			<command title="balanceSheet" />
			<command title="cashFlowReport" />
			<command title="trialBalance" />
			<command title="transactionDetailByAccount" />
			<command title="expenseReport" />
			<command if="trackTax" title="transactionDetailedbyTaxItem" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>{Customers}andReceivablesReports</input>
		<input>{customer}reports</input>
		<outputs>
			<text name="salesReports" />
			<command title="arAgeingSummary" />
			<command title="arAgeingDetail" />
			<command title="{Customer}Statement" />
			<command title="{Customer}TransactionHistory" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>salesReports</input>
		<outputs>
			<text name="salesReports" />
			<command title="salesBy{Customer}Summary" />
			<command title="salesBy{Customer}Detail" />
			<command title="salesByItemSummary" />
			<command title="salesByItemDetail" />
			<!-- <command title="Sales Order Report" /> -->
		</outputs>
	</pattern>

	<pattern login="true">
		<input>{Vendors}andPayableReports</input>
		<input>{vendor}reports</input>
		<outputs>
			<command title="apAgeingSummary" />
			<command title="apAgeingDetail" />
		</outputs>
	</pattern>

	<pattern login="true">
		<input>purchasesReports</input>
		<outputs>
			<text name="purchasesReports" />
			<command title="purchaseBy{Vendor}Summary" />
			<command title="purchaseBy{Vendor}Detail" />
			<command title="purchaseByItemSummary" />
			<command title="purchaseByItemDetail" />
			<!-- <command title="Purchase Order Report" /> -->
		</outputs>
	</pattern>

	<pattern login="true" if="trackTax">
		<input>vATReports</input>
		<input>tAXReports</input>
		<outputs>
			<text name="VAT Reports" />
			<command if="isUK" title="priorVATReturns" />
			<command if="isUK" title="vatDetail" />
			<command if="isNotUK" title="taxItemDetailReport" />
			<command if="isUK" title="vat100" />
			<command if="isUK" title="vatExceptionDetail" />
			<command if="isNotUK" title="taxItemExceptionDetailReport" />
			<command if="isUK" title="uncategorizedVATAmounts" />
			<command if="isUK" title="ecSalesList" />
			<command if="isUK" title="vATItemSummary" />
			<command if="isNotUK" title="vatItemSummary" />
		</outputs>
	</pattern>
</patterns>